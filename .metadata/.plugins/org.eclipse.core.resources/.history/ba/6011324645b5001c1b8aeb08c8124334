
public class Principal {

	public static void main(String[] args) {

		int auxiliar = 0;
		for (int i = 1; i <= 100; i++) {

			if (i % 3 == 0) {
				System.out.println("Os múltiplos são:");
				System.out.println(i);
				System.out.println();
				auxiliar++;
			}

		}

		System.out.println("O três tem " + auxiliar + " múltiplos!!");

	}

}
