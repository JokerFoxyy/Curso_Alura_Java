public class Teste_IR {

	public static void main(String[] args) {

		/*
		 * De 1900.0 até 2800.0, o IR é de 7.5% e pode deduzir na declaração o valor de
		 * R$ 142 De 2800.01 até 3751.0, o IR é de 15% e pode deduzir R$ 350 De 3751.01
		 * até 4664.00, o IR é de 22.5% e pode deduzir R$ 636
		 */

		double salarioBruto = 3300.0, salarioLiquido;

		if (salario >= 1900 && salario <= 2800) {
			salarioLiquido = salario - 142;
		} else if (salario > 2800 && salario <= 3751) {
			salarioLiquido = salario - 350;

		} else if (salario < 4664) {
			salarioLiquido = salario - 636;
		}

		// ifs aqui
	}
}
